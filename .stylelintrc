{
  "rules": {
    "declaration-no-important": true,
    "no-empty-source": [
      true,
      {
        "severity": "warning"
      }
    ],
    "unit-no-unknown": [
      true,
      {
        "severity": "warning"
      }
    ],
    "property-no-unknown": [
      true,
      {
        "severity": "warning"
      }
    ],
    "no-extra-semicolons": [
      true,
      {
        "severity": "warning"
      }
    ]
  },
  "extends": [
    "stylelint-config-recommended"
  ]
}
